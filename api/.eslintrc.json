{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "no-underscore-dangle": 0,
      "camelcase": 0,
      "dot-notation": 0,
      "max-len": 0
  }
}